package com.liuzg.jswebextra.utils;

import com.liuzg.jswebextra.plugins.wxattention.model.WXUserData;
import org.json.JSONObject;

/**
 * Created by NEUNB_Lisy on 2017/10/30.
 */
public class WXAttenUtil {
    public static WXUserData jsonTowxUserData(JSONObject jsonObject){
        WXUserData wxUserData = new WXUserData();
        wxUserData.setOpenid(String.valueOf(jsonObject.get("openid")));
        wxUserData.setNickname(String.valueOf(jsonObject.get("nickname")));
        wxUserData.setSex(String.valueOf(jsonObject.get("sex")));
        wxUserData.setLanguage(String.valueOf(jsonObject.get("language")));
        wxUserData.setCity(String.valueOf(jsonObject.get("city")));
        wxUserData.setProvince(String.valueOf(jsonObject.get("province")));
        wxUserData.setCountry(String.valueOf(jsonObject.get("country")));
        wxUserData.setHeadimgurl(String.valueOf(jsonObject.get("headimgurl")));
        wxUserData.setPrivilege(String.valueOf(jsonObject.get("privilege")));
        wxUserData.setUnionid(String.valueOf(jsonObject.get("unionid")));
        return wxUserData;
    }
}
